<launch>
<group ns ="front">
  <node name="rplidarNode"          pkg="rplidar_ros"  type="rplidarNode" output="screen">
  <param name="serial_port"         type="string" value="/dev/frplidar"/>
  <param name="serial_baudrate"     type="int"    value="115200"/>
  <param name="frame_id"            type="string" value="front_laser"/>
  <param name="inverted"            type="bool"   value="false"/>
  <param name="angle_compensate"    type="bool"   value="true"/>
  </node>
</group>
<node pkg="tf" type="static_transform_publisher" name="base_link_to_laser_broadcaster_f" args="0.11 0 0.29 3.14 0 3.141592 base_link front_laser 50" />
</launch>
